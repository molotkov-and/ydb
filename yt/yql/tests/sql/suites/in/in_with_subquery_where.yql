-- Disable most providers to canonize easily
USE plato;

$subq = (SELECT subkey FROM Input WHERE value = "ddd");

SELECT
    key as key,
    "" as subkey,
    "value:" || value as value
FROM Input
WHERE subkey in ($subq)
ORDER BY key
